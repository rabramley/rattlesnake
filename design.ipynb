    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x=np.arange(-1, 1, step = 0.02)\n",
    "plt.ylim(-1, 1)\n",
    "arctan=np.arctan(x*2)*2/np.pi\n",
    "plt.plot(x,arctan)\n",
    "plt.plot(x,x)\n",
    "\n",
    "plt.show()\n"
   ]
